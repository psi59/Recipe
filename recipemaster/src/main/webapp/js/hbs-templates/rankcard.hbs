{{#each data}}
        {{#if @first}}    
          <li class="list_divide first">
            <div class="list_item top_item1">
        {{else if @last}}    
          <li class="list_divide last">
           <div class="list_item top_item3">
        {{else}}   
          <li class="list_divide">
            <div class="list_item top_item2">
        {{/if}}
                  <span class="rank"><em></em></span>
                  <div class="thmb_profile">
                    <div class="thmb">
                      <img src="img/profileImg/{{user.image}}" alt=""> <span
                        class="mask"></span>
                    </div>
                  </div>
                  <div class="list_body">
                    <div class="writer_sec">
                      <span class="writer no_badge"><em class="ellipsis">{{user.userName}}</em></span>
                    </div>
                    <p class="desc ellipsis">{{user.intro}}</p>
                  </div>
              </div>
              <div class="list_side">

                <div class="recommend ">
              <button class="_cheerUp _param('longzhe1983')" onclick="nhn.Kin.Utility.nClicks('hfh.kglike', '', '', event);">
                <input type="hidden" class="rcp-hidden-email" value="{{user.email}}">  
              <span class="num _count rcp-userName">페이지로이동</span>
              </button>
            </div>
              </div>
            </li>
{{/each}}